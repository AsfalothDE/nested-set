{
    "name": "spiritix/nested-set",
    "type": "library",
    "description": "A comprehensive library implementing the nested set pattern in PHP using PDO.",
    "license": "MIT",
    "keywords": ["php","pdo","nested-set","nested","set"],
    "authors": [
        {
            "name": "Matthias Isler",
            "email": "mi@matthias-isler.ch"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Spiritix\\NestedSet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiritix\\NestedSet\\Tests\\": "tests"
        }
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
